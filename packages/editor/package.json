{
  "name": "@edumetz16/firecms_editor",
  "version": "3.0.0-canary.53",
  "description": "Notion-style WYSIWYG editor for FireCMS",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly -p tsconfig.prod.json",
    "clean": "rm -rf dist && find ./src -name '*.js' -type f | xargs rm -f"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@edumetz16/firecms_ui": "^3.0.0-canary.53",
    "@radix-ui/react-slot": "^1.0.2",
    "@tiptap/core": "^2.3.1",
    "@tiptap/extension-color": "^2.3.1",
    "@tiptap/extension-highlight": "^2.3.1",
    "@tiptap/extension-horizontal-rule": "^2.3.1",
    "@tiptap/extension-image": "^2.3.1",
    "@tiptap/extension-link": "^2.3.1",
    "@tiptap/extension-placeholder": "2.3.1",
    "@tiptap/extension-task-item": "^2.3.1",
    "@tiptap/extension-task-list": "^2.3.1",
    "@tiptap/extension-text-style": "^2.3.1",
    "@tiptap/extension-underline": "^2.3.1",
    "@tiptap/pm": "^2.3.1",
    "@tiptap/react": "^2.3.1",
    "@tiptap/starter-kit": "^2.3.1",
    "@tiptap/suggestion": "^2.3.1",
    "@types/node": "20.12.11",
    "cmdk": "^0.2.1",
    "jotai": "^2.8.0",
    "react-markdown": "^9.0.1",
    "react-moveable": "^0.56.0",
    "tippy.js": "^6.3.7",
    "tiptap-markdown": "^0.8.10",
    "tunnel-rat": "^0.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "18.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "gitHead": "5d6f89260a9582eaac7ee9a77c48c8f340bc29d3"
}
